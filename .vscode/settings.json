{
  "json.schemas": [
    {
      "fileMatch": [
        "renovate.json5"
      ],
      "url": "https://docs.renovatebot.com/renovate-schema.json"
    }
  ],
  "eslint.validate": [
    "json",
    "jsonc",
    "json5"
  ],
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "chat.tools.terminal.autoApprove": {
    "rm": false,
    "rmdir": false,
    "del": false,
    "kill": false,
    "curl": true,
    "wget": true,
    "eval": true,
    "chmod": true,
    "chown": true,
    "/^Remove-Item\\b/i": false,
    "cd": true,
    "make": true,
    "python3": true,
    "find": true,
    "git commit": false,
    "git": true,
    "wc": true,
    "mv": true
  },
  "yaml.schemas": {
    "https://www.schemastore.org/yamllint.json": [
      "file:///workspaces/openshift-v2.0/cluster.yaml",
      "file:///workspaces/openshift/charts/media/sonarr/templates/service.yaml"
    ]
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  }
}
